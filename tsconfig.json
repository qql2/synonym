{
  "compilerOptions": {
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "CommonJS",
    "target": "ES6",
    "allowJs": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "esModuleInterop":true,
    "lib": [
      "DOM",
      "ES5",
      "ES6",
      "ES7"
    ],
  },
  "include": [
    "**/*.ts"
  ],
  // "exclude": ["**/*.test.ts","**/*.test.js"],
}
